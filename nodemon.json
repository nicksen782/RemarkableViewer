{
	"restartable": "rs",
	"ignore": [
		".git",
		"node_modules/**/node_modules",
		"./DEVICE_DATA",
		"./DEVICE_DATA_IMAGES"
	],
	"verbose": false,
	"execMap": {
		"js": "node "
	},
	"events": {
		"restart": ""
	},
	"watch": [
		"nodemon.json",
		"configFile.json", 
		"_backend.js", 
		"modules/*.js"
	],
	"env": {
		"NODE_ENV": "development"
	},
	"ext": "js",
	"delay": "2000"
}


